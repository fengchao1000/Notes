@page
@inherits FC.Notes.Web.Pages.NotesPage
@using Volo.Abp.AspNetCore.Mvc.UI.Bootstrap.TagHelpers.Modal
@model FC.Notes.Web.Pages.Categorys.CreateModalModel
@{
    Layout = null;
}
<abp-dynamic-form abp-model="Category" data-ajaxForm="true" asp-page="/Categorys/CreateModal">
    <abp-modal>
        <abp-modal-header title="@L["NewCategory"].Value"></abp-modal-header>
        <abp-modal-body>
            <abp-form-content />
        </abp-modal-body>
        <abp-modal-footer buttons="@(AbpModalButtons.Cancel|AbpModalButtons.Save)"></abp-modal-footer>
    </abp-modal>
</abp-dynamic-form>